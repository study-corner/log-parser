{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "jms/serializer": "^3.10",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
