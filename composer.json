{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "doctrine/orm": "^2.7",
        "jms/serializer": "^3.10",
        "phpunit/phpunit": "^9.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
